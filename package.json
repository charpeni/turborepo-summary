{
  "name": "turborepo-summary",
  "version": "1.0.1",
  "description": "Generate a human-readable summary report from Turborepo run summary JSON output",
  "keywords": [
    "turborepo",
    "turbo",
    "summary",
    "report",
    "markdown",
    "json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/charpeni/turborepo-summary.git"
  },
  "license": "MIT",
  "author": "Nicolas Charpentier <nicolas.charpentier079@gmail.com>",
  "type": "module",
  "bin": "./dist/index.js",
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "package.json"
  ],
  "scripts": {
    "build": "tsdown",
    "format": "prettier --write .",
    "lint": "eslint .",
    "prepack": "pnpm build",
    "prepare": "husky",
    "test": "jest",
    "type-check": "tsc --pretty --noEmit"
  },
  "lint-staged": {
    "*.{js,ts,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "commander": "^14.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/jest": "^30.0.0",
    "eslint": "^9.38.0",
    "eslint-plugin-import": "^2.32.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.5",
    "prettier": "3.6.2",
    "ts-jest": "^29.4.5",
    "tsdown": "^0.15.9",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2"
  },
  "packageManager": "pnpm@10.19.0"
}
